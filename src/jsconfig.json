{
    "compilerOptions": {
        "target": "ES6",
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "components/*": [
                "./components/*"
            ],
            "containers/*": [
                "./containers/*"
            ],
            "resources/*": [
                "./resources/*"
            ],
            "assets/*": [
                "./assets/*"
            ],
            "commons/*": [
                "./commons/*"
            ],
        }
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}